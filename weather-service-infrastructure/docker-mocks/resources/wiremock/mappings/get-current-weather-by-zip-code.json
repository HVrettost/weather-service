{
  "priority": 2,
  "request": {
    "method": "GET",
    "urlPath": "/weather",
    "queryParameters": {
      "zip": {
        "matches": "[0-9]+"
      },
      "appid": {
        "equalTo": "c5bca6977c807a27776c8a36988e68c3"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "current-weather-by-zip-code.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}