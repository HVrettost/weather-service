{
  "priority": 3,
  "request": {
    "method": "GET",
    "urlPath": "/weather",
    "queryParameters": {
      "q": {
        "matches": "[A-Za-z ]+"
      },
      "appid": {
        "equalTo": "dummy-owm-app-id"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "current-weather/current-weather-by-city.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}